<div class="container">
  <%= link_to 'Back', lists_path, class: 'btn btn-secondary mb-4 mt-3' %>
  <h1>Add new list</h1>
  <%= simple_form_for @list do |f| %>
    <%= f.input :name, autofocus: true %>
    <%= f.input :photo, as: :file %>
    <%= f.submit class: 'btn btn-primary' %>
  <% end %>
  </div>
</div>
